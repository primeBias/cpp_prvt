syntax on
filetype plugin indent on		"Enable file type based indentation.
set autoindent
set shiftwidth=4
set backspace=2

colorscheme murphy
set foldmethod=syntax
set tabstop=4
set number
set hlsearch
set incsearch
let g:netrw_liststyle = 3


set wildmenu						"enables tab autocomplete
set wildmode=list:longest,full		" complete to the longest string
									" then open wildmenu
